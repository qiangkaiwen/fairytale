    "serve": "npm run build; npm run start",
    "build": "NODE_ENV=production npm i; npm run build:client; npm run build:server",
    "start": "node ./dist/server",
    "build:client": "BABEL_ENV=client webpack --config webpack.client.prod.js",
    "build:server": "BABEL_ENV=server webpack --config webpack.server.prod.js",
    "dev": "node ./node_modules/babel-cli/bin/babel-node.js ./src/dev-server.js",
    "build:dev": "BABEL_ENV=client  NODE_ENV=development webpack --config webpack.config.dev.js",
    "lint": "eslint ./src/**",
    "refresh": "rm -rf node_modules; rm package-lock.json; npm i"
  },
